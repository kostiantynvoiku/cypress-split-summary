{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es5", "dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["cypress"]
    },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}